{
   "name": "anim-3d-obj",
   "version": "2.0.0",
   "description": "Configurable 3D object with animated rotations and face content",
   "license": "MIT",
   "author": "mdnelles",
   "sideEffects": [
      "dist/**/*.css",
      "src/styles/obj.css"
   ],
   "module": "dist/esm/index.js",
   "main": "dist/cjs/index.cjs",
   "types": "dist/esm/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/esm/index.js",
         "require": "./dist/cjs/index.cjs",
         "types": "./dist/esm/index.d.ts"
      },
      "./components/Obj": {
         "import": "./dist/esm/components/Obj.js",
         "require": "./dist/cjs/components/Obj.cjs",
         "types": "./dist/esm/components/Obj.d.ts"
      },
      "./dist/cjs/components/Obj": "./dist/cjs/components/Obj.cjs"
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "build": "tsup",
      "clean": "rm -rf dist",
      "dev": "tsup --watch",
      "prepublishOnly": "npm run clean && npm run build"
   },
   "peerDependencies": {
      "react": ">=17 || >=18",
      "react-dom": ">=17 || >=18"
   },
   "devDependencies": {
      "@types/react": "^18.3.3",
      "@types/react-dom": "^18.3.0",
      "tsup": "^8.0.1",
      "typescript": "^5.5.4"
   }
}
